{
  "manifest_version": 2,
  "name": "Page visit count",
  "description": "Get page visit count",
  "version": "0.1",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs", "history"]
}
